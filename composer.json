{
    "name": "pifeifei/easy-api",
    "type": "library",
    "description": "easy api client.",
    "keywords": [
        "easy",
        "api",
        "client"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "composer/ca-bundle": "^1.2",
        "guzzlehttp/guzzle": "^6.5|^7.3",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "symfony/cache": "^3.4|^4.3|^5.0",
        "symfony/serializer": "^3.4|^4.3|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^3.4|^4.0|^5.0",
        "vlucas/phpdotenv": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Pff\\EasyApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pff\\EasyApiTest\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kisÈæç",
            "email": "pifeifei1989@qq.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
